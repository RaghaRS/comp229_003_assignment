<%-include partials/header.ejs%>
  <%-include partials/main_nav.ejs%>

    <main class="container pb-5">
      <div class="py-5 text-center">

        <h2>Contact Me</h2>
        <p class="lead">To contact me send an email to : rsundar7@my.centennialcollege.ca</p>
      </div>

      <div class="g-5 justify-content-center row">

        <div class="col-md-7 col-lg-8">

          <form action="/" method="redirect" role="form">
            <div class="row g-3">
              <div class="col-sm-6">
                <label for="firstName" class="form-label">First name</label>
                <input type="text" class="form-control" id="firstName" placeholder="" value="" required="">

              </div>

              <div class="col-sm-6">
                <label for="lastName" class="form-label">Last name</label>
                <input type="text" class="form-control" id="lastName" placeholder="" value="" required="">

              </div>



              <div class="col-12">
                <label for="contact" class="form-label">Contact</label>
                <input type="tel" class="form-control" id="contact" required="">

              </div>
              <div class="col-12">
                <label for="email" class="form-label">Email </label>
                <input type="email" class="form-control" id="email" placeholder="you@example.com">

              </div>



              <div class="col-12">
                <label for="address" class="form-label">Address</label>
                <textarea class="form-control" aria-label="With textarea"></textarea>

              </div>






            </div>















            <hr class="my-4">

            <button class="w-100 btn btn-dark btn-lg mb-5" type="submit">submit</button>
          </form>
        </div>
      </div>
    </main>



    <!--Script section-->
    <%-include partials/bottom_nav.ejs%>
